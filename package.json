{
  "name": "http-ffmpeg-server",
  "version": "1.0.0",
  "license": "MIT",
  "bin": {
    "http-ffmpeg-server": "./dist/index.js"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production node build.js",
    "build:dev": "cross-env NODE_ENV=development node build.js",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "start:dev": "cross-env NODE_ENV=development node ./dist/index.js",
    "dev": "yarn build:dev && yarn start:dev",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "pkg": "yarn build && pkg --config pkg.config.json --compress GZip --output ./dist/http-ffmpeg-server ./dist/index.js"
  },
  "dependencies": {
    "async-lock": "^1.3.0",
    "bluebird": "^3.7.2",
    "execa": "^5.1.1",
    "express": "^5.0.0",
    "ffmpeg-static": "^4.4.0",
    "fs-capacitor": "^7.0.1",
    "pump": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tmp": "^0.2.4"
  },
  "devDependencies": {
    "@types/async-lock": "^1.1.3",
    "@types/bluebird": "^3.5.36",
    "@types/express": "^4.17.13",
    "@types/ffmpeg-static": "^3.0.0",
    "@types/node": "^16.4.9",
    "@types/pump": "^1.1.1",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/tmp": "^0.2.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.0",
    "husky": "^7.0.1",
    "typescript": "^4.3.5"
  }
}
